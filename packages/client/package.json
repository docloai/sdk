{
  "name": "@doclo/client",
  "version": "0.1.1",
  "description": "Doclo cloud client for executing flows via API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "@doclo/core": "^0.1.7",
    "@doclo/flows": "^0.1.0",
    "@doclo/prompts": "^0.1.0",
    "@doclo/schemas": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@doclo/flows": {
      "optional": true
    },
    "@doclo/prompts": {
      "optional": true
    },
    "@doclo/schemas": {
      "optional": true
    }
  },
  "devDependencies": {
    "@doclo/core": "workspace:*",
    "@doclo/flows": "workspace:*",
    "@doclo/prompts": "workspace:*",
    "@doclo/providers-llm": "workspace:*",
    "@doclo/schemas": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "doclo",
    "document",
    "extraction",
    "ocr",
    "llm",
    "api",
    "client"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/docloai/sdk.git",
    "directory": "packages/client"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
