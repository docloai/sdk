{
  "name": "@docloai/schemas",
  "version": "0.1.1",
  "description": "JSON Schema definitions for document extraction types",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/docloai/sdk.git",
    "directory": "packages/schemas"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./schemas/*": "./schemas/*",
    "./schemas/*.json": "./schemas/*.json"
  },
  "files": [
    "dist",
    "schemas"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@docloai/prompts": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "@types/node": "^20"
  }
}
